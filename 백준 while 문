while True:
    a,b=map(int,input().split())
    if a+b==0:
        break
    print(a+b)
    


# while True:
#     a,b=map(int,input().split())
#     print(a+b)


i=0
n=int(input())
a=str(n)
while True :
    if 0<n<10:
        n=int(str(n)[-1])*10+int(str(n)[-1])
    else:
        n=int(str(n)[-1])*10+int(str(int(str(n)[0])+int(str(n)[-1]))[-1])
    i+=1
    if n==int(a) :
        print(i)
        break
